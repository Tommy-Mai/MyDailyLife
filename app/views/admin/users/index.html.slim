.users-index
  .container
    h1.form-heading ユーザー一覧
    - @users.each do |user|
      .users-index-item
        .user-right 
          p= user.name
        - if user.protected == false
          = link_to '削除',"/admin/users/#{user.id}/destroy", method: :delete, data:{ confirm: "ユーザー「#{user.name}」を削除します。よろしいですか？"}
